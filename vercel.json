{
  "redirects": [
    { "source": "/", "destination": "/index.html", "status": 200 },
    { "source": "/*", "destination": "/index.html", "status": 200 },
    { "source": "/(.*)", "destination": "/index.html", "status": 200 }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31556952, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "github": {
    "enabled": true
  }
}
