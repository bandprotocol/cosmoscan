{
  "redirects": [{ "source": "/.*", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31556952, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "github": {
    "enabled": true
  }
}
